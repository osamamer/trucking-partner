{
  "version": 2,
  "builds": [
    {
      "src": "../backend/vercel_app.py",
      "use": "@vercel/python"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "../backend/vercel_app.py"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ]
}